        ]
    },
    {
        "description": "CAPS_LOCK to control when pressed with any other key, to ESC when alone.",
        "manipulators": [
            {
                "type": "basic",
                "from": {
                    "key_code": "caps_lock",
                    "modifiers": {
                        "optional": [
                            "any"
                        ]
                    }
                },
                "to": [
                    {
                        "set_variable": {
                            "name": "caps_lock pressed",
                            "value": 1
                        }
                    },
                    {
                        "key_code": "left_control",
                        "lazy": true
                    }
                ],
                "to_if_alone": [{
                    "key_code": "escape"
                }],
                "to_after_key_up": [{
                    "set_variable": {
                        "name": "caps_lock pressed",
                        "value": 0
                    }
                }]
            }
        ]
    },
        {
            "description": "ESC changed to CAPS_LOCK if held down",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "escape",
                        "modifiers": {
                            "optional": ["any"]
                        }
                    },
                    "parameters": {
                        "basic.to_if_alone_timeout_milliseconds": 250,
                        "basic.to_if_held_down_threshold_milliseconds": 250
                    },
                    "to_if_alone": [
                        {
                            "key_code": "escape"
                        }
                    ],
                    "to_if_held_down": [
                        {
                            "key_code": "caps_lock"
                        }
                    ]
                }
            ]
    },
        {
        "description": "Fn + hjkl to arrow keys",
        "manipulators": [
            {
            "type": "basic",
