{
    "title": "aifusenno1 Personal Setting",
    "rules": [
    {
        "description": "CAPS_LOCK+arrows to fn+arrows,  (w/ modifiers). Must also enable the main CAPS_LOCK setiting.",
        "manipulators": [
            {
            "type": "basic",
            "from": {
                "key_code": "left_arrow",
                "modifiers": {
                    "mandatory": [
                        "left_control"
                    ],
                    "optional": [
                        "left_shift",
                        "left_option",
                        "left_command"
                    ]
                }
            },
            "to": [{
                "key_code": "left_arrow",
                "modifiers": "fn"
            }],
            "conditions": [{
                "type": "variable_if",
                "name": "caps_lock pressed",
                "value": 1
            }]
        },
            {
            "type": "basic",
            "from": {
                "key_code": "right_arrow",
                "modifiers": {
                    "mandatory": [
                        "left_control"
                    ],
                    "optional": [
                        "left_shift",
                        "left_option",
                        "left_command"
                    ]
                }
            },
            "to": [{
                "key_code": "right_arrow",
                "modifiers": "fn"
            }],
            "conditions": [{
                "type": "variable_if",
                "name": "caps_lock pressed",
                "value": 1
            }]
        },
            {
            "type": "basic",
            "from": {
                "key_code": "up_arrow",
                "modifiers": {
                    "mandatory": [
                        "left_control"
                    ],
                    "optional": [
                        "left_shift",
                        "left_option",
                        "left_command"
                    ]
                }
            },
            "to": [{
                "key_code": "up_arrow",
                "modifiers": "fn"
            }],
            "conditions": [{
                "type": "variable_if",
                "name": "caps_lock pressed",
                "value": 1
            }]
        },
            {
            "type": "basic",
            "from": {
                "key_code": "down_arrow",
                "modifiers": {
                    "mandatory": [
                        "left_control"
                    ],
                    "optional": [
                        "left_shift",
                        "left_option",
                        "left_command"
                    ]
                }
            },
            "to": [{
                "key_code": "down_arrow",
                "modifiers": "fn"
            }],
            "conditions": [{
                "type": "variable_if",
                "name": "caps_lock pressed",
                "value": 1
            }]
        }
        ]
    },
